{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "stripInternal": true,
        "importHelpers": false,
        "strictNullChecks": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": true,
        "sourceMap": false,
        "outDir": "lib",
        "lib": ["dom", "es2015", "es5"]
    },
    "files": [
        "src/cli.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}